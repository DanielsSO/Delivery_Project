<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Views.infoEnviosView">

    <TableView fx:id="tableView" prefHeight="800" prefWidth="1500">
        <columns>
            <TableColumn fx:id="colNombreEnvia" text="Quien Envía" prefWidth="150"/>
            <TableColumn fx:id="colDireccionEnvia" text="Direccion" prefWidth="250"/>
            <TableColumn fx:id="colOtrosEnvia" text="Otros Datos Envío" prefWidth="150"/>
            <TableColumn fx:id="colNombreRecibe" text="Quien Recibe" prefWidth="150"/>
            <TableColumn fx:id="colDireccionRecibe" text="Direccion" prefWidth="250"/>
            <TableColumn fx:id="colOtrosRecibe" text="Otros Datos Recibe" prefWidth="150"/>
            <TableColumn fx:id="colNumeroRastreo" text="Número de Rastreo" prefWidth="150"/>
            <TableColumn fx:id="colFechaEnvio" text="Fecha de Envío" prefWidth="150"/>
        </columns>
    </TableView>

    <Label fx:id="lblInicio" text="Inicio" style="-fx-font-size: 18px;" AnchorPane.topAnchor="2" AnchorPane.rightAnchor="50"/>
    <ImageView fx:id="menuView" fitWidth="30" fitHeight="25" pickOnBounds="true" AnchorPane.topAnchor="2" AnchorPane.rightAnchor="10" onMouseClicked="#volver"/>

</AnchorPane>
